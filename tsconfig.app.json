{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
  "module": "ESNext",
  "jsx": "react-jsx",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "convex/react": ["./convex/react.ts"],
      "../../convex/_generated/api": ["./convex/_generated/api.ts"],
      "../../convex/_generated/api.js": ["./convex/_generated/api.ts"],
      "../../convex/_generated/server": ["./convex/_generated/server.ts"],
      "../../convex/_generated/server.js": ["./convex/_generated/server.ts"]
    },
    "ignoreDeprecations": "6.0"
  },
  "include": ["node_modules/src"]
}
